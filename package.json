{
  "name": "@opencollective/taxes",
  "version": "3.0.0",
  "description": "Helpers to calculate taxes on Open Collective",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint . --ext='js,ts'",
    "lint:check": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "prettier": "prettier \"**/*.@(js|ts|json|md)\" --ignore-path .eslintignore",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "type:check": "tsc"
  },
  "keywords": [
    "tax",
    "vat",
    "open-collective"
  ],
  "author": "Benjamin Piouffle",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "babel-eslint": "10.1.0",
    "codecov": "3.8.2",
    "eslint": "7.29.0",
    "eslint-config-opencollective": "2.2.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "25.5.4",
    "jest-junit": "12.2.0",
    "prettier": "2.1.2",
    "ts-jest": "25.5.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "jsvat": "2.2.0",
    "lodash": "^4.17.20",
    "vatrates": "2.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.history/"
    ],
    "globals": {
      "ts-jest": {
        "warnOnly": true
      }
    }
  }
}
